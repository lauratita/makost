	<queryString>
		<![CDATA[SELECT * FROM tbl_pembayaran 
JOIN tbl_detailpembayaran ON tbl_pembayaran.no_transaksi = tbl_detailpembayaran.no_transaksi
JOIN tbl_manajemen_kamar ON tbl_pembayaran.id_manajemen = tbl_manajemen_kamar.id_manajemen
